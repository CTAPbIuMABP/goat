{
	"parser": "babel-eslint",
	"extends": "airbnb",
	"rules": {
		"indent": [
			2,
			"tab"
		],
		"comma-dangle": [
			2,
			"never"
		],
		"quotes": [
			2,
			"double"
		],
		"object-curly-spacing": [
			2,
			"never"
		],
		"space-before-function-paren": [
			2,
			"never"
		],
		"padded-blocks": [
			0,
			"never"
		],
		"no-else-return": [
			0
		],
		"react/jsx-indent-props": [
			2,
			"tab"
		]
	},
	"env": {
		"mocha": true
	}
}
