  "scripts": {
    "postinstall": "rimraf ./build && babel -d ./build/server ./server --copy-files && babel -d ./build/client ./client --ignore build --copy-files",
    "build": "webpack -c --config server/configs/webpack.js",
    "db": "NODE_ENV=${NODE_ENV:=test} babel-node tools/db-category.js",
    "lint": "eslint source/* test/* --quiet --fix",
    "port": "lsof -t -i tcp:8080 | xargs kill",
    "prod": "NODE_ENV=${NODE_ENV:=production} babel-node source/server.js",
    "start:pm2": "pm2 start ecosystem.json --env dev",
    "start:nodemon": "NODE_ENV=${NODE_ENV:=development} TWITTER_API=${TWITTER_API:=true} DEBUG=web:* nodemon server/blog.js",
    "stop": "pm2 stop --watch all; pm2 delete all;",
    "test": "NODE_ENV=${NODE_ENV:=test} ABL_DEBUG=${ABL_DEBUG:=info} babel-node node_modules/.bin/_mocha",
    "manual": "NODE_ENV=${NODE_ENV:=test} ABL_DEBUG=${ABL_DEBUG:=info} node_modules/.bin/babel-node",
    "coverage": "NODE_ENV=${NODE_ENV:=test} node_modules/.bin/babel-node node_modules/istanbul/lib/cli cover node_modules/.bin/_mocha test/routes/* test/connect test/utils test/controllers/*",
    "travis": "babel-node node_modules/istanbul/lib/cli cover node_modules/.bin/_mocha test/routes/* test/connect test/utils test/controllers/*"
  }
